pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                // Exemple de commande pour le build
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                // Exemple de commande pour les tests
                sh 'mvn test'
            }
        }
    }

    // Sp√©cifie le chemin vers le Jenkinsfile dans backoffice
    options {
        scm {
            scriptPath('backoffice/Jenkinsfile')
        }
    }
}
