pipeline {

        stage('Récupération du code de la branche') {
            steps {

                    git branch: 'master', 
                    url: 'https://github.com/AchrafBouslama/SopraHRProject.git'


            }
        } 


        stage('Build') {
            steps {
                 dir('backoffice') {
                // Exemple de commande pour le build
                sh 'mvn clean package'
                 }
            }
        }
        stage('Test') {
            steps {
                 dir('backoffice') {
                // Exemple de commande pour les tests
                sh 'mvn test'
                 }
            }
        }
    }

